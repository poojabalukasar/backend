{
  "version": 2,
  "builds": [
    {
      "src": "shop/backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "shop/backend/server.js"
    }
  ],
  "projectSettings": {
    "framework": {
      "defaultBuildScript": "npm run build",
      "buildCommand": "cd shop && npm install && npm run build"
    }
  }
}
